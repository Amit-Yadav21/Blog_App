<html>
<head>
  <title>User Registration</title>
  <link rel="stylesheet" href="update.css">
</head>
<body>
  <div class="main">
    <% if (error) { %>
      <script>
              alert('<%= error %> <%= additionalText %>'); // Display the error message and additional text in an alert
      </script>
    <% } %>

    <h1>Update User Profile</h1>
    <form action="/updatedata" method="POST" enctype="multipart/form-data">
      <div>
  
        <input type="file" name="image" placeholder="update image">
      </div>
      <div class="button">

        <button type="submit">Update</button>
         <a href="http://localhost:4000/login">login</a>
      </div>
    </form>
  </div>
</body>
</html>